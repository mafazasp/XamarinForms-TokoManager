<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TokoManager.Views.HomePage"
             Title="User Info"
             NavigationPage.HasBackButton="False">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="This is Home Page!"/>
            <Button x:Name="userpagebtn" Text="User Options" Command="{Binding ToUserPageCommand}" />
            <Button x:Name="logoutbtn" Text="Logout" Command="{Binding LogoutCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>